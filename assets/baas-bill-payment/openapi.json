    "/bill-payments/{payment_guuid}": {
      "post": {
        "tags": [
          "Bill Payment"
        ],
        "description": "Make a payment and return information information about operation.",
        "parameters": [
          {
            "in": "path",
            "name": "payment_guuid",
            "schema": {
              "type": "string",
              "format": "uuid"
            },
            "required": true,
            "description": "Transaction ID for authorized bill payment."
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "payer": {
                    "type": "object",
                    "properties": {
                      "document_type": {
                        "type": "string",
                        "enum": [
                          "CPF",
                          "CNPJ"
                        ]
                      },
                      "document_number": {
                        "type": "number"
                      }
                    }
                  },
                  "amount": {
                    "type": "number",
                    "format": "float"
                  }
                },
                "required": [
                  "payer - document_type - document_number",
                  "amount"
                ],
                "example": {
                  "payer": {
                    "document_type": "CPF",
                    "document_number": "1234567890"
                  },
                  "amount": 123.48
                }
              }
            }
          }
        },
